{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-crypto-pipeline"
		},
		"ln_cryptopipestorage_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://cryptopipestorage.blob.core.windows.net/"
		},
		"ls_crypopipestoorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cryptopipestorage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-crypto-pipeline_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ln_cryptopipestorage_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('ln_cryptopipestorage_properties_typeProperties_serviceEndpoint')]"
					},
					"ls_crypopipestoorage_properties_typeProperties_url": {
						"value": "[parameters('ls_crypopipestoorage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}